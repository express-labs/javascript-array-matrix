{
   "presets": [
  		["env", {"modules": false}],
  		"react"
  	],
   "plugins": [
      "transform-class-properties",
      "transform-object-rest-spread",
      "transform-object-assign",
      "external-helpers"
   ],
   "env": {
      "test": {
         "plugins": [
            ["transform-runtime", { "runtimeHelpers": true }],
            "transform-es2015-modules-commonjs"
         ]
      }
   }
}
